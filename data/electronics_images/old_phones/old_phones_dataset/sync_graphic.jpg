<script type="text/javascript">
    (
        function (document, window, index) {
    var wScrollBefore	= 300,
        wScrollCurrent	= window.pageYOffset,
        wScrollDiff		= 0;

    var hasScrolledLogo = false;
    var hasScrolledBar = false;

    var globalNavTop = 0;
    var globalNavBar = document.getElementsByClassName("flat-nav")[0];
    var globalNavPlaceholder = document.getElementsByClassName("flat-nav-placeholder")[0];
    var globalNavBackground = document.getElementsByClassName("flat-nav-background")[0];
    var globalNavParent = document.getElementsByClassName("flat-nav-parent")[0];
    var cobrandBar = document.getElementsByTagName("cui-cobrand-bar");
    var bannerBar = document.getElementsByTagName("cui-mobile-app-banner");
    var carsLogoParent = document.getElementsByClassName("global-nav__logo")[0];
    var carsLogo = document.getElementById("cars-com-logo");
    var angularProfileComponent = document.getElementsByClassName("global-nav__profile-trigger");
    var globalNavHamburgerMenu = document.getElementById("global-nav__menu-trigger");
    var mobileTriggerController = document.getElementById("mobile-trigger-controller");

    const HOME_PATH = "/";

    const FRAMES_PER_SECOND = 30;
    const FRAMES_PER_SECOND_MILLIS = 1000 / FRAMES_PER_SECOND;

    const CARS_LOGO_HEIGHT = 66;
    var headerLogoDifference = CARS_LOGO_HEIGHT - globalNavBar.getBoundingClientRect().height;
    var globalNavBoundsCheck = globalNavBar.getBoundingClientRect().height + headerLogoDifference;

    function hideMobileMenu() {
        mobileTriggerController.checked = false;
    };

    // Set the hamburger menu to closed when we navigate away from the page
    if ("onpagehide" in window) {
        window.addEventListener("pagehide", hideMobileMenu);
    } else {
        window.addEventListener("beforeunload", hideMobileMenu);
    }

    checkLogoAnimation();

    addProfileScrollLockFix();
    addWindowScrollEvent();
    addIsHomeClass();

    if (cobrandBar.length > 0) {
        adjustHeaderToElement(cobrandBar[0]);
    }
    if (bannerBar.length > 0) {
        adjustHeaderToElement(bannerBar[0]);
    }

    // Firefox MMY fix for negatively positioned elements
    function checkNegativeBounds(element) {
        return (element.getBoundingClientRect().top < 0);
    }

    function addIsHomeClass() {
        if (window.location.pathname == HOME_PATH) {
            carsLogoParent.classList.add("home-logo-top");
        }
    }

    function checkLogoAnimation() {
        if (wScrollCurrent > globalNavTop + CARS_LOGO_HEIGHT) {
            carsLogo.classList.add('logo-small');
            carsLogo.classList.remove('logo-big');
            hasScrolledLogo = true;
        } else if (hasScrolledLogo) {
            carsLogo.classList.remove('logo-small');
            carsLogo.classList.add('logo-big');
        }
    }

    function checkHeaderPositionStyle(headerPositionCheck) {
        globalNavBackground.style.top = globalNavTop + "px";

        // Scrolled to top, global nav becomes part of page
        if (wScrollCurrent <= headerPositionCheck) {
            globalNavPlaceholder.style.display = "none";
            globalNavBar.style.position = "relative";
        } else if (hasScrolledBar) {
            globalNavPlaceholder.style.display = "initial";
            globalNavBar.style.position = "fixed";
        }
    }

    function adjustBarDefaultPosition(elementHeight) {
        globalNavTop = elementHeight;
        checkHeaderPositionStyle(globalNavTop);
    }

    function adjustHeaderToElement(element) {
        if (checkNegativeBounds(element)) {
            return;
        }

        adjustBarDefaultPosition(element.getBoundingClientRect().height);

        element.addEventListener("DOMSubtreeModified", function() {
            adjustBarDefaultPosition(element.getBoundingClientRect().height);
        }, false);
    }

    function addProfileScrollLockFix() {
        if (angularProfileComponent.length > 0) {
            function unclickNavMenu(event) {
                setTimeout(function() {
                    mobileTriggerController.checked = false;
                }, 0);
            };

            angularProfileComponent[0].addEventListener("click", unclickNavMenu);
        }
    }

    function addWindowScrollEvent() {

        function scrollFunction() {
            wScrollCurrent = window.pageYOffset;
            wScrollDiff = wScrollBefore - wScrollCurrent;

            var totalSizeCheck = globalNavTop + globalNavBoundsCheck;

            var scrolledUp = wScrollDiff > 0;

            if (mobileTriggerController.checked) {
                globalNavBar.style.position = "fixed";
                globalNavBar.style.top = "0px";
            } else {
                if (scrolledUp) {
                    globalNavBar.classList.remove('header--hidden');
                    // If we scrolled passed the total header height, set hasScrolledBar flag to true
                    hasScrolledBar = (wScrollCurrent > totalSizeCheck);
                } else if (wScrollCurrent > totalSizeCheck) {   // If we scrolled down and are below the header position, hide it
                    globalNavBar.classList.add('header--hidden');
                }

                checkLogoAnimation();
                checkHeaderPositionStyle(globalNavTop);
            }

            wScrollBefore = wScrollCurrent;
        };

        // Lodash doesn't seem to be loaded when this runs, so we have to add a timeout
        setTimeout(function() {
            if (typeof _ != "undefined") {
                window.addEventListener('scroll', _.throttle(scrollFunction, FRAMES_PER_SECOND_MILLIS));
            } else {
                window.addEventListener('scroll', scrollFunction);
            }
        }, 1000);
    }
}
        ( document, window, 0 )
    );
</script>

